import expect from 'expect';

import * as Flux from '../src/reduxRest';

describe('Flux', () => {
  const APIConf = {endpoint: 'enpoint/'};
  
  it('should create an API helper for each API endpoint', () => {
    let flux = new Flux(APIConf);
    expect(flux.API.endpoint).toBeAn(Flux.Endpoint);
  });
});

